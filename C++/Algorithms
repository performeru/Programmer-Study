#include <iostream>

void Print(int input[], int count)
{
	for(int i=0; i<count; i++)
	{
		std::cout << input[i] << " ";
	}
	std::cout << std::endl;
}

void Swap(int& value, int& value2)
{
	int temp = value;

	value = value2;
	value2 = temp;
}

int main()
{	
	const int ARRAY{ 5 };
	int array[ARRAY]{ 8,7,2,3,1 };

	Print(array, ARRAY);
}
